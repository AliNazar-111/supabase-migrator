# Supabase Migrator Environment Template

# Source Database (PostgreSQL URL)
# Example: postgresql://postgres:password@db.source-project.supabase.co:5432/postgres
SOURCE_CONNECTION_STRING=

# Target Database (PostgreSQL URL)
# Example: postgresql://postgres:password@db.target-project.supabase.co:5432/postgres
TARGET_CONNECTION_STRING=

# Supabase Project Details (Required for storage bucket migration)
# Get these from Project Settings > API
SOURCE_SUPABASE_URL=
SOURCE_SERVICE_ROLE_KEY=

TARGET_SUPABASE_URL=
TARGET_SERVICE_ROLE_KEY=

# Optional Settings
DEFAULT_SCHEMA=public
EXPORT_BATCH_SIZE=1000
OUTPUT_DIR=./supabase-migrator
